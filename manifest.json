{
	"name":"uyghurche",
	"version":"1.0",
	"manifest_version":2,
	"description":"an chrome extension for uyghur language...",
	"icons":{
		"16":"images/icon.png",
		"32":"images/icon.png",
		"48":"images/icon.png",
		"128":"images/icon.png"
	},
	"permissions": [
		"tabs", 
		"activeTab",
		"background",
		"http://*/*", 
		"https://*/*",
	    "tabs",  
	    "notifications",  
	    "http://localhost:8080/" 
	],
	"browser_action":{
		"default_icon":"images/icon.png",
		"default_title":"uyghurche",
		"default_popup":"popup.html"
	},
	"content_scripts": [
	    {
	      "matches": ["http://www.aaaaa.com/*"],
	      "js": ["popup.js"]
	    }
  	]
}